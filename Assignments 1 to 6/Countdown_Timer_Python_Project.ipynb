        "from os import cpu_count\n",
        "import time\n",
        "\n",
        "def count(t):\n",
        "  while t:\n",
        "    mins,secs= divmod(t,60)\n",
        "    timer='{:02d}:{:02d}'.format(mins,secs)\n",
        "    print(timer,end=\"\\r\")\n",
        "    time.sleep(1)\n",
        "    t -= 1\n",
        "\n",
        "  print(\"Timer Comp;eted\")\n",
        "t =input(\"ENter the time in seconds\")\n",
        "count(int(t))\n",
        "\n",
        "\n",
        "\n"
      ]
